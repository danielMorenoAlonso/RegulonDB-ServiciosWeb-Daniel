{
  "name": "regulondb-serviciosweb",
  "version": "1.0.0",
  "engines": {
    "node": "14.15.0"
  },
  "description": "Servicios web para herramientas  RegulonDB implementando graphql",
  "main": "gateway.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "npm run build && node build/closedTools/closedTools.js",
    "start:all": "concurrently \"npm run start:dev\" \"npm run start:dev:OpenTools\" ",
    "start:OpenTools": "npm run build && node build/openToolsService/openTools.js",
    "start:dev": "nodemon --exec babel-node src/closedTools/closedTools.js",
    "start:dev:OpenTools": "nodemon --exec babel-node src/openToolsService/openTools.js",
    "build": "babel src -d build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regulondbunam/RegulonDB-ServiciosWeb-Daniel.git"
  },
  "author": "Daniel Moreno Alonso",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/regulondbunam/RegulonDB-ServiciosWeb-Daniel/issues"
  },
  "homepage": "https://github.com/regulondbunam/RegulonDB-ServiciosWeb-Daniel#readme",
  "dependencies": {
    "@apollo/federation": "^0.20.7",
    "@apollo/gateway": "^0.21.4",
    "apollo-server-express": "^2.19.0",
    "axios": "^0.21.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "graphql": "^15.4.0",
    "graphql-import": "^1.0.2",
    "merge-graphql-schemas": "^1.7.8",
    "mongoose": "^5.10.11"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
